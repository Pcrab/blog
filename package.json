        "start": "astro dev",
        "syncpack:fix": "syncpack format && syncpack fix-mismatches",
        "prepare": "husky"
    },
    "type": "module",
    "main": "index.js",
    "dependencies": {
        "@astrojs/sitemap": "^3.1.6",
